<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.picsdream.picsdreamsdk">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:supportsRtl="true">
        <activity android:name=".activity.InitialDataLoadActivity"
            android:noHistory="true"/>
        <activity
            android:name="com.paytm.pgsdk.PaytmPGActivity"
            android:configChanges="keyboardHidden|orientation|keyboard"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.ReviewOrderActivity"
            android:windowSoftInputMode="adjustNothing"/>
        <activity android:name=".activity.AddressActivity"/>
        <activity android:name=".activity.PaymentActivity"
            android:noHistory="true"/>
        <activity android:name=".activity.SelectCountryActivity"
            android:noHistory="true"/>
        <activity android:name=".activity.PrefsActivity"/>
        <activity android:name=".activity.ProductsActivity"/>
        <activity android:name=".activity.PurchaseActivity"
            android:noHistory="true"/>
        <activity android:name=".activity.SizeGuideActivity"
            android:noHistory="true"/>
        <activity android:name=".activity.EmailPhotoActivity"
            android:noHistory="true"
            android:windowSoftInputMode="adjustResize"/>
    </application>
</manifest>
